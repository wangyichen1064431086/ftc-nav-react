{
  "env": {
    "development": {
      "presets": ["es2015", "react","stage-0"],
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy"
      ]
    },
    "testing": {
      "presets": ["es2015", "react","stage-0"],
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy"
      ]
    },
    "production": {
      "presets": [
        ["es2015",{"modules": false}],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "external-helpers"
      ]
    }
  }
  
}